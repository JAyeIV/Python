stock_price = float(input('stock_price: '))
EPS = float(input('EPS: '))
pe_ratio = float(stock_price) / float(EPS)
print("P/E ratio: " + f'{pe_ratio}')

total_assets = float(input('total_assets: '))
total_liabilities = float(input('total liabilities: '))
asset_debt_ratio = float(total_assets) / float(total_liabilities)
print("Assets to liabilities ratio: " + f'{asset_debt_ratio}')

market_cap = float(input('market_cap: '))
net_income = float(input('net_income: '))
market_cap_income_ratio = float(market_cap) / float(net_income)
print("Market cap to income ratio: " + f'{market_cap_income_ratio}')
